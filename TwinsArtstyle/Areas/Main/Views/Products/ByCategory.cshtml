@model IEnumerable<ProductViewModel>

@{
    ViewData["Title"] = "Products";
}

<div id="products">
    <h3>@Model.FirstOrDefault().Category</h3>
    <div class="panel">

        @foreach (var item in Model)
        {
            <partial name="_ProductPartial" model="item" />
        }

    </div>
</div>