@model IEnumerable<UserViewModel>
@{
    ViewData["Title"] = "Registered Users";
}

<div id="registered-users">
    <div class="panel">
        <h3>Latest Registrations</h3>
        <table id="users">
            <thead>
                <tr class="row">
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Email</th>
                    <th>Phone Number</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var user in Model)
                {
                    <tr class="row">
                        <td>@user.FirstName</td>
                        <td>@user.LastName</td>
                        <td>@user.Email</td>
                        <td>@user.PhoneNumber</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>